<Window
    x:Class="Material.Components.Gallery.Views.ShellView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mat="https://https://github.com/pedrior/material-components-wpf"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Material Components for WPF"
    Width="1280"
    Height="720"
    MinWidth="896"
    MinHeight="504"
    d:DesignHeight="1080"
    d:DesignWidth="1920"
    Background="{DynamicResource MaterialColorSurface}"
    FontFamily="{DynamicResource MaterialFontRoboto}"
    SnapsToDevicePixels="True"
    UseLayoutRounding="True"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d" />
